{
    "name": "amphp/redis",
    "description": "Async Redis client.",
    "keywords": [
        "amp",
        "amphp",
        "async",
        "client",
        "redis"
    ],
    "license": "MIT",
    "homepage": "https://github.com/amphp/redis",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com",
            "role": "Creator / Lead Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/amphp/redis/issues"
    },
    "require": {
        "php": ">=7.0",
        "amphp/amp": "dev-master as 2.0",
        "amphp/socket": "dev-amp_v2 as 1.0"
    },
    "require-dev": {
        "amphp/loop": "dev-master",
        "phpunit/phpunit": "^4.8|^5.2",
        "fabpot/php-cs-fixer": "^1.9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Amp\\Redis\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Redis\\": "test"
        }
    }
}
